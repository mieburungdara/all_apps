# Contoh file .env
# Salin file ini ke .env dan isi nilai-nilai yang sesuai

# Konfigurasi Aplikasi
APP_NAME="My Application"
APP_URL="http://localhost:8000"
APP_ENV="development"
APP_DEBUG=true

# Konfigurasi Database (jika menggunakan .env)
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=your_database
DB_USERNAME=your_username
DB_PASSWORD=your_password

# Konfigurasi Google Auth (jika menggunakan .env)
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""
GOOGLE_REDIRECT_URI=""

# Konfigurasi Telegram (jika menggunakan .env)
TELEGRAM_BOT_TOKEN=""
TELEGRAM_CHAT_ID=""
